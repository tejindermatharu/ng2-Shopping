{ "compileOnSave": true,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "types": ["jasmine"],
    "typeRoots" : ["./typings"],
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom","es6" ],
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true
  },
 "include": [
        "app/**/*"
    ]
}
